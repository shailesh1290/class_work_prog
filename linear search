#include<stdio.h>
int main(){
    int n,arr[99],i,value ,in,count = 0,pos;

    printf("enter n = ");
    scanf("%d",&n);

    printf("enter array:\n");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }


printf("enter value = ");
scanf("%d",&value);


   for(i=0;i<n;i++){
        if(arr[i]==value){
            count++;
    }


}
if(count==1){
    printf("num is IN array:");
}
else{
    printf("number is NOT IN array:");
}
}
