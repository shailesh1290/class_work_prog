#include<stdio.h>
#include<string.h>
int main(){
  int a ,b;

 printf("enter a = ");
  scanf("%d",&a);

    
 printf("enter b = ");
  scanf("%d",&b);

printf("\n");
    printf("sum  is = %d\n",a+b);
    printf("mul  is = %d\n",a*b);
    printf("sub  is = %d\n",a-b);
    printf("divide is = %d\n",a%b);
    printf("mod  is = %d\n",a/b);
}
